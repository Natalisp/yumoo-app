<% if !@yelp_restaurants.empty? %>
  <table class="ui table">
    <thead>
      <tr>
      <th>Yelp Restaurants</th>
      <th>Price Range</th>
      <th>Category</th>
      <th>Phone</th>
      <th>Address</th>
      <th>Restaurant Rating</th>
      </tr>
    </thead>
    <tbody>
      <%= render @yelp_restaurants %>
    </tbody>
  </table>
<% end %>

